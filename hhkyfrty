def successCountTXpercent = ctx.payload.aggregations.aggregate_terms.buckets.success_percent_tx.value; def successCountWApercent = ctx.payload.aggregations.aggregate_terms.buckets.success_percent_wa.value; def successCountMApercent = ctx.payload.aggregations.aggregate_terms.buckets.success_percent_ma.value; if (successCountTXpercent == null || successCountWApercent == null || successCountMApercent == null) { return false; } return successCountTXpercent < thresholdshort || successCountTXpercent > thresholdlong || successCountWApercent < thresholdshort || successCountWApercent > thresholdlong || successCountMApercent < thresholdshort || successCountMApercent > thresholdlong;"
