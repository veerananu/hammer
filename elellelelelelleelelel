def threshold = 10; return ctx.payload != null && ctx.payload.aggregations != null && ctx.payload.aggregations.aggregate_terms != null && ctx.payload.aggregations.aggregate_terms.buckets != null && ctx.payload.aggregations.aggregate_terms.buckets.stream().filter(bucket -> bucket.data != null && bucket.data.buckets != null && bucket.data.buckets.long != null && bucket.data.buckets.short != null && bucket.data.buckets.long.success_percent != null && bucket.data.buckets.short.success_percent != null && bucket.data.buckets.long.success_percent.value > threshold && bucket.data.buckets.short.success_percent.value > threshold).count() > 0;
